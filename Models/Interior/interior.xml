<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>cadre</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intvitres</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <path>Aircraft/Macchi-M33/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m>  0.147 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.646 </z-m>
    </offsets>
  </model>

  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/Macchi-M33/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -0.202 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.418 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -25 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

</PropertyList>

